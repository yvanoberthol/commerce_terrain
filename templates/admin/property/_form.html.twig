{{ form_start(form) }}
<div class="row">
    {{ form_errors(form) }}
</div>
<div class="row mt-4">
    <div class="col-md-4">
        {{ form_row(form.title) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.surface) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.rooms) }}
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        {{ form_row(form.bedrooms) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.floor) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.price) }}
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        {{ form_row(form.heat) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.city) }}
    </div>
    <div class="col-md-4">
        {{ form_row(form.address) }}
    </div>
</div>
{{ form_rest(form) }}
<button type="submit" class="btn btn-primary">
    <span class="fa fa-{{ icon }}"></span> {{ button|default('Do') }}
</button>
<a href="{{ path('admin.property.index') }}" class="ml-4">Retour Ã  la liste des biens</a>
{{ form_end(form) }}